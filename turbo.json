{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".svelte-kit/**"]
    },
    "clean:all": {
      "dependsOn": ["^clean:all"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "tsconfig.json",
    ".env.*"
  ]
}
